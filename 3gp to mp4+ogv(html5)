ffmpeg -i ./INPUTINPUTINPUT.3gp -r 29.97 -vcodec libxvid -b 700k -acodec libfaac -ab 128k -bufsize 4M -qmax 51 -mbd 2 -cmp 2 -subcmp 2 -ar 44100 -g 300 -s 320x240 -aspect 4:3 -ac 2 -f mp4 -y OUTPUTOUTPUTOUTPUT.mp4 && USE OGGCONVERT


