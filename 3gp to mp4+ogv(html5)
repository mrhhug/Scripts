ffmpeg -i sample.avi -strict experimental -acodec vorbis -vcodec libtheora sample.ogv

ffmpeg -i sample.avi sample.ogv -strict experimental -acodec vorbis -vcodec theora -s 640x480 -f ogg  

ffmpeg -i sample.avi samdple.ogv -strict experimental -acodec vp8 -vcodec theora -s 640x480


